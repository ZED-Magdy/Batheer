@model IEnumerable<Batheer.Application.TargetedSchedulingForProjectImplementations.Queries.GetTargetedSchedulingForProjectImplementations.TargetedSchedulingForProjectImplementation>


@using Batheer.WebUI.Utls;
@{
    ViewData["Title"] = "Index";
}



<h1>الجدولة الزمنية المستهدفة لتنفيذ كل مشروع </h1>

<form class="form">
    <div class="form-group row">
        <label for="inputEmail3" class="col-sm-2 col-form-label">اسم الجمعية</label>
        <div class="col-sm-10">
            <input type="email" class="form-control" id="inputEmail3" placeholder="Email" value="جمعية أحد المسارحة">
        </div>
    </div>

    <div class="form-group row">
        <label for="inputEmail3" class="col-sm-2 col-form-label">اسم المشروع</label>
        <div class="col-sm-10">
            <input type="email" class="form-control" id="inputEmail3" placeholder="Email" value="مشروع رقم 1">
        </div>
    </div>


</form>

<table class="table table-striped">
    <thead>
        <tr>
            <th>
                اسم المشروع
            </th>
            <th>
                تصنيف المشروع
            </th>
            <th>
                من تاريخ
            </th>
            <th>
                إلى تاريخ
            </th>
            <th>
                حالة المشروع
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ProjectName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CouncilProjectName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FromDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ToDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TargetedSchedulingForProjectImplementationStatusName)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">المستهدفين من المشروع</a>
                </td>
            </tr>
        }
    </tbody>
</table>
