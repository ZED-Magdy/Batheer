@model Batheer.Application.ExcludeIdentityNumbers.Commands.CreateExcludeIdentityNumber.CreateExcludeIdentityNumberCommand

<div class="col-lg-12">
    <form asp-action="Create">
        <div class="row">
            <h4>إضافة إلى القائمة</h4>
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group col-md-5">
                <label asp-for="IdentityNo" class="control-label">رقم الهوية</label>
                <input asp-for="IdentityNo" class="form-control" required />
                <span asp-validation-for="IdentityNo" class="text-danger"></span>
            </div>

            <div class="form-group col-md-5">
                <label asp-for="Notes" class="control-label">ملاحظات</label>
                <textarea asp-for="Notes" class="form-control" rows="1"></textarea>
            </div>

            <div class="form-group col-md-2">
                <br />
                <input type="submit" value="إضافة" class="btn btn-primary" />
            </div>
        </div>

    </form>
</div>


