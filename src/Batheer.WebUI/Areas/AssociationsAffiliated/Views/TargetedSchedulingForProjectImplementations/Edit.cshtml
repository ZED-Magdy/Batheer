@model Batheer.Application.TargetedSchedulingForProjectImplementations.Queries.GetById.TargetedSchedulingForProjectImplementation

@{
    ViewData["Title"] = "Edit";
}


<div class="row">
    <div class="col-lg-12">
        <div class="card">
            @*<div class="card-header">Form Elements</div>*@
            <div class="card-body">
                <h1>تعديل</h1>

                <h4>الجدولة الزمنية للمشروع</h4>
                <hr />
                <div class="col-md-4">
                    <form asp-action="Edit">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <input type="hidden" asp-for="Id" />
                        <div class="form-group">
                            <label asp-for="AssociationAffiliatedProjectId" class="control-label">اسم المشروع</label>
                            <select asp-for="AssociationAffiliatedProjectId" class="form-control" asp-items="ViewBag.AssociationAffiliatedProjectId"></select>
                        </div>
                        <div class="form-group">
                            <label asp-for="FromDate" class="control-label">من تاريخ</label>
                            <input asp-for="FromDate" class="form-control" />
                            <span asp-validation-for="FromDate" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="ToDate" class="control-label">إلى تاريخ</label>
                            <input asp-for="ToDate" class="form-control" />
                            <span asp-validation-for="ToDate" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="TargetedSchedulingForProjectImplementationStatusId" class="control-label">حالة المشروع</label>
                            <select asp-for="TargetedSchedulingForProjectImplementationStatusId" class="form-control" asp-items="ViewBag.TargetedSchedulingForProjectImplementationStatusId"></select>
                        </div>
                        <div class="form-group">
                            <input type="submit" value="حفظ" class="btn btn-primary" />
                            <a asp-action="Index" class="btn btn-default">عودة</a>
                        </div>
                    </form>
                </div>

                <div>
                </div>

            </div>
        </div>
    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
